          </item>
          <item row="3" column="1">
           <widget class="QCheckBox" name="pPLim">
            <property name="text">
             <string>Keep lower/upper restrictions for amounts of phases </string>
            </property>
           </widget>
          </item>
          <item row="4" column="0">
           <widget class="QCheckBox" name="pXr0h0">
            <property name="text">
             <string>Keep size parameters r0, h0 of solid particles (pores), reserved</string>
            </property>
