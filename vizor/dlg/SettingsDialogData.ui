        <receiver>SettingsDialogData</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>pButtonCancel</sender>
        <signal>clicked()</signal>
        <receiver>SettingsDialogData</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>pButtonHelp</sender>
        <signal>clicked()</signal>
        <receiver>SettingsDialogData</receiver>
        <slot>CmHelp()</slot>
    </connection>
    <connection>
        <sender>pButtonChFont</sender>
        <signal>clicked()</signal>
        <receiver>SettingsDialogData</receiver>
        <slot>CmChangeFont()</slot>
    </connection>
    <connection>
